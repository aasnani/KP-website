{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "css/",
    "images/",
    "pages/",
    "*.html",
    "server.js"
  ],
  "env": {},
  "ext": "js,json,css, png, webp, jpg, jpeg"
}